{
  "name": "doshio",
  "version": "1.0.0",
  "description": "the ultimate anime and manga picker",
  "main": "server.ts",
  "repository": "git@github.com:nicolasanaia/mangime.git",
  "author": "Nicolas Anaia <nicolasanaia@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "start": "npm run build --scripts-prepend-node-path && cross-env NODE_ENV=production node dist/server.js",
    "build:tsc": "tsc && tsc-alias",
    "migrate:make": "knex migrate:make --knexfile src/database/knexfile.ts -x ts",
    "migrate:latest": "dotenv -e .env.development.local -- knex migrate:latest --knexfile src/database/knexfile.ts",
    "migrate:up": "dotenv -e .env.development.local -- knex migrate:up --knexfile src/database/knexfile.ts",
    "migrate:rollback": "dotenv -e .env.development.local -- knex migrate:rollback --knexfile src/database/knexfile.ts"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv-cli": "^7.3.0",
    "express": "^4.18.2",
    "knex": "^3.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.4",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.3",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.6.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "mysql2": "^3.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
